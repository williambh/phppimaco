{
    "name": "williamla/phppimaco",
    "description": "Lib para geração de Etiquetas PIMACO, atualizado para PHP 8.1",
    "keywords": ["PIMACO", "etiquetas","tags","hang tags"],
    "license": "MIT",
    "authors": [
        {
            "name": "Willian Leite Araújo",
            "email": "william.bh@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Williamla\\PhpPimaco\\": "src/"
        }
    },
    "require": {
        "php": ">=7.0",
        "ext-json": "*",
        "mpdf/mpdf": "^8.0",
        "endroid/qr-code": "^2.5",
        "picqer/php-barcode-generator": "^0.4"
    },
    "autoload-dev": {
        "psr-4": {
            "Williamla\\PhpPimacoTest\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "squizlabs/php_codesniffer": "^3.2"
    }
}
